{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/change-clothing",
      "dest": "/backend/api/change-clothing.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/$1"
    }
  ]
}
