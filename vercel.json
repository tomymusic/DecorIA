{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/api/redesign-room$",
      "dest": "/backend/api/redesign-room.js"
    },
    {
      "src": "^/api/(.*)$",
      "dest": "/backend/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
